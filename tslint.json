{
	"defaultSeverity": "warning",
	"extends": [
		"tslint:recommended"
	],
	"linterOptions": {
		"exclude": [
			"node_modules/**"
		]
	},
	"rules": {
		"eofline": true,
		"semicolon": [
			true, 
			"always"
		],
		"indent": [
			true,
			"tabs"
		],
		"interface-name": false,
		"ordered-imports": false,
		"object-literal-sort-keys": false,
		"no-consecutive-blank-lines": false,
		"trailing-comma": false
	}
}